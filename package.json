{
  "name": "fabricator-esx",
  "version": "1.3.1",
  "description": "A tool for building website UI toolkits based off of http://fbrctr.github.io/",
  "license": "MIT",
  "homepage": "http://fbrctr.github.io/",
  "scripts": {
    "prestart": "npm install",
    "build": "npm run prestart && gulp",
    "buildforcejssourcemap": "npm run prestart && gulp --jsSourcemap",
    "start": "gulp --dev",
    "test": "npm run buildforcejssourcemap",
    "yprestart": "yarn install",
    "ybuild": "yarn yprestart && gulp",
    "ybuildforcejssourcemap": "yarn yprestart && gulp --jsSourcemap",
    "ystart": "gulp --dev",
    "ytest": "yarn ybuildforcejssourcemap"
  },
  "keywords": [
    "toolkit",
    "styleguide",
    "framework",
    "scaffold",
    "patterns",
    "library"
  ],
  "author": "Carl Paradis",
  "contributors": [
    "Carl Paradis <paresthesia69@gmail.com> (http://carlparadis.io)"
  ],
  "licenses": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/paresthesia69/fabricator-esx"
  },
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0.0"
  },
  "eslintConfig": {
    "globals": {
      "window": true
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "browser-sync": "^2.13.0",
    "del": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "fabricator-assemble": "^1.1.11",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-csso": "^3.0.1",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.7",
    "imports-loader": "^0.8.0",
    "run-sequence": "^2.2.1",
    "script-loader": "^0.7.0",
    "webpack": "^4.1.1"
  }
}
